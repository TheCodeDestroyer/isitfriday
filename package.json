{
  "name": "isitfriday",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "serve": "next start",
    "base:prettier": "prettier './**/*.{ts,tsx,html,json,md,css}' --config .prettierrc --cache",
    "base:eslint": "eslint ./ --ext .ts,.tsx  --cache",
    "lint:prettier": "pnpm base:prettier --check",
    "lint:eslint": "pnpm base:eslint",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "format:prettier": "pnpm base:prettier --write",
    "format:eslint": "pnpm base:eslint --fix",
    "format": "pnpm format:prettier && pnpm format:eslint"
  },
  "engines": {
    "node": "18.x.x",
    "pnpm": "8.x.x"
  },
  "dependencies": {
    "axios": "1.4.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.4.4",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/lodash": "4.14.194",
    "@types/node": "18.16.16",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-tcd": "1.3.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "^2.6.2",
    "typescript": "5.0.4"
  }
}
