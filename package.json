{
  "name": "isitfriday",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "base:prettier": "prettier './**/*.{ts,tsx,html,json,md,css,svg}' --config .prettierrc --cache",
    "base:eslint": "eslint ./ --ext .js,.ts,.tsx --cache",
    "lint:prettier": "pnpm base:prettier --check",
    "lint:eslint": "pnpm base:eslint",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "format:prettier": "pnpm base:prettier --write",
    "format:eslint": "pnpm base:eslint --fix",
    "format": "pnpm format:prettier && pnpm format:eslint",
    "type:check": "tsc --noEmit"
  },
  "engines": {
    "node": "20.x.x",
    "pnpm": "9.x"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "next": "14.2.33",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.2.33",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash": "4.17.20",
    "@types/node": "20.19.25",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-config-tcd": "2.0.4",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "postcss": "8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "3.4.18",
    "typescript": "5.9.3"
  }
}
