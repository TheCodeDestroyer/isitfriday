{
  "name": "isitfriday",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "base:eslint": "eslint ./ --ext .js,.ts,.tsx --cache",
    "base:prettier": "prettier './**/*.{ts,tsx,html,json,md,css,svg}' --config .prettierrc --cache",
    "build": "next build",
    "dev": "next dev",
    "format": "pnpm format:prettier && pnpm format:eslint",
    "format:eslint": "pnpm base:eslint --fix",
    "format:prettier": "pnpm base:prettier --write",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:eslint": "pnpm base:eslint",
    "lint:prettier": "pnpm base:prettier --check",
    "start": "next start",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "next": "16.0.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@tcd-devkit/eslint-preset-next": "0.1.13",
    "@tcd-devkit/prettier-config-react": "0.1.7",
    "@tcd-devkit/tsconfig": "0.1.3",
    "@types/lodash": "4.17.21",
    "@types/node": "20.19.25",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "eslint": "9.39.1",
    "postcss": "8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.25.0",
  "engines": {
    "node": "20.x.x",
    "pnpm": "10.x"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
