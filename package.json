{
  "name": "isitfriday",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "base:prettier": "prettier './**/*.{ts,tsx,html,json,md,css,svg}' --config .prettierrc --cache",
    "base:eslint": "eslint ./ --ext .js,.ts,.tsx --cache",
    "lint:prettier": "pnpm base:prettier --check",
    "lint:eslint": "pnpm base:eslint",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "format:prettier": "pnpm base:prettier --write",
    "format:eslint": "pnpm base:eslint --fix",
    "format": "pnpm format:prettier && pnpm format:eslint",
    "type:check": "tsc --noEmit"
  },
  "engines": {
    "node": "20.x.x",
    "pnpm": "8.x.x"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.33.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.2.3",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash": "4.17.4",
    "@types/node": "20.14.1",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-tcd": "1.3.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.38",
    "prettier": "^3.0.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}
